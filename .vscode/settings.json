{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".vscode": true,
        ".next": true,
        "node_modules": true,
        "package-lock.json": true,
        "next-env.d.ts": true,
        "next.config.ts": true,
        "eslint.config.mjs": true,
        "postcss.config.mjs": true,
        "tsconfig.json": true,
        ".gitignore": true
    },
    "hide-files.files": [
        ".vscode",
        ".next",
        "node_modules",
        "package-lock.json",
        "next-env.d.ts",
        "next.config.ts",
        "eslint.config.mjs",
